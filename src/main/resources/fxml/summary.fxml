<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="200.0" prefWidth="300.0"
            xmlns="http://javafx.com/javafx/9.0.1"
            fx:controller="pl.agh.edu.wiet.to2.kevin.controllers.SummaryController">
    <Label layoutX="14.0" layoutY="14.0" prefHeight="17.0" prefWidth="183.0" text="Ilość poprawnych odpowiedzi: "/>
    <Label layoutX="14.0" layoutY="34.0" prefHeight="17.0" prefWidth="244.0"
           text="Ilość częściowo poprawnych odpowiedzi:"/>
    <Label layoutX="14.0" layoutY="54.0" prefHeight="17.0" prefWidth="244.0" text="Ilość błędnych odpowiedzi:"/>
    <Label layoutX="14.0" layoutY="74.0" prefHeight="17.0" prefWidth="244.0" text="Punkty:"/>
    <Button alignment="TOP_CENTER" layoutX="80.0" layoutY="117.0" mnemonicParsing="false"
            onAction="#onMenuButtonClicked" prefHeight="25.0" prefWidth="140.0" text="Menu"/>
    <Label layoutX="258.0" layoutY="14.0" text="${controller.gameStatistics.correctAnswers}"/>
    <Label layoutX="258.0" layoutY="34.0" text="${controller.gameStatistics.partiallyCorrectAnswers}"/>
    <Label layoutX="258.0" layoutY="54.0" text="${controller.gameStatistics.wrongAnswers}"/>
    <Label layoutX="258.0" layoutY="74.0" text="${controller.gameStatistics.points}"/>
</AnchorPane>
